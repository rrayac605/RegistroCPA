			<EAScenario name="FB Opinión sin salvedad con pago en una exhibición" type="Basic Path" weight="0.00" subject="EAID_28CD7220_F08B_45ed_8B2F_C354F8B9BF0A" xmi.id="EAID_71B45CF8_0CA6_4aa1_B36D_27BC7496974E">
				<EAScenarioContent>
					<path>
						<step name="El CPA selecciona la opción de Opinión en la pantalla de Formulación de Dictamen." guid="{C4462B08-BC72-4a12-8E4E-C674BC55C772}" level="1" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El sistema muestra en pantalla RFC002 y el campo para seleccionar el tipo de opinión. RFC100&#xA;" guid="{3DF2E637-CA78-4991-9504-F53350B515B3}" level="2" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="El CPA selecciona la opción de Opinión que quiere emitir. Se llama FA02, FA03, FA04 o en caso de seleccionar Sin Salvedad  continua en el paso 4.  RFN002." guid="{F0C14717-D177-4511-B238-03BEE09EA6EE}" level="3" uses="" useslist="" result="" state="" trigger="1" link="">
							<extension level="3a" guid="{1EFA3F5E-A1F3-4d40-B41B-8DDF98B1339A}" join="End"/>
							<extension level="3b" guid="{33FBAD2A-FD85-46a6-BF89-28E749F3D972}" join="End"/>
							<extension level="3c" guid="{D5BBF086-4102-40a1-845E-30D8A7B0928E}" join="End"/>
						</step>
						<step name="El sistema despliega en pantalla las opciones de pago RFC200" guid="{A5A93602-3C7E-400b-8C6D-8A2C605DC7C5}" level="4" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="El CPA elige la opción referente a la aplicación o no de pago." guid="{D10D3F46-0423-4da9-AC63-F3863EA44975}" level="5" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="En caso de aplicar pago el sistema habilita el campo Forma de pago y la información correspondiente a cada forma. RFC300 y RFC400, RFC006" guid="{3059D5AF-DC01-4891-8749-DDEF803C2259}" level="6" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="El CPA registra en el cuadro de observaciones información adicional. RFC007" guid="{8C161AC6-C745-40df-BFDC-7F8DACB74B5A}" level="7" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El caso que el  CPA requiera ver la vista previa de su opinión ver FA05." guid="{6216EF13-1C85-4b56-AF3C-6D860DDEAE7C}" level="8" uses="" useslist="" result="" state="" trigger="1" link="">
							<extension level="8a" guid="{36B4354A-6459-4de8-A2DA-5B3D516107D8}" join="End"/>
						</step>
						<step name="El CPA da clic en el botón Firmar Trámite." guid="{0D0E329B-8780-4419-BC3B-DB93327631EE}" level="9" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El sistema después de validar se cumpla despliega la ventana del componente de firma digital,  FAC600" guid="{5CB6A1D9-5221-4a88-A79B-C55793927E49}" level="10" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="El usuario captura y selecciona los archivos correspondientes a la firma electrónica y da clic en el botón Firmar." guid="{A4ECB361-1EC1-44ae-8429-01430418C65B}" level="11" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El sistema realiza la firma del trámite y genera el acuse respectivo. RFC008.&#xA;El sistema despliega en PDF el acuse, lo almacena en la notaria electrónica y lo envía por correo electrónico al CPA y al Patrón." guid="{44B3C3F6-5D36-4f45-941C-FE1FA360F705}" level="12" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="Termina caso de uso" guid="{9BB8B2A1-ED95-44ac-9790-B212C238D437}" level="13" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<context>
							<item oldname="RFC002 - Encabezado de pantalla con detalle información del patron a dictaminar&#xA;" guid="{053744D6-C8D3-4888-98E6-7B514AB79E3F}"/>
