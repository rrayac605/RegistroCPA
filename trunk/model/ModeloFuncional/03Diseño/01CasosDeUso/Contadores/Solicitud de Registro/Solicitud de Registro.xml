				<EAScenarioContent>
					<path>
						<step name="El usuario selecciona la opción Contadores públicos de trámites digitales." guid="{AF292051-E8C1-47c9-A6B3-BCCFE23BAA7A}" level="1" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El sistema muestra en pantalla las opciones del detalle 1 del RF-C002. Detalle de Pantallas" guid="{04C5CFFB-C23F-4624-A898-0A60A7F48A07}" level="2" uses="" useslist="" result="" state="" trigger="0" link=""/>
