			<EAScenario name="EX004. Error en carga de BD" type="Exception" weight="2.00" subject="EAID_E041F09B_720D_4718_94C3_E8907CC31B52" xmi.id="EAID_31AAD483_F7BB_45ee_8EE8_69A9ABF02131">
