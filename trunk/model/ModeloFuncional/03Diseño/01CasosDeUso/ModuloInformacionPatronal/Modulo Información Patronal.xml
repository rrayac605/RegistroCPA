			<EAScenario name="FA001. Eliminar archivo" type="Alternate" weight="5.00" subject="EAID_E041F09B_720D_4718_94C3_E8907CC31B52" xmi.id="EAID_6F99C4B6_3649_47f3_AC24_EA7EAD759CCA">
				<EAScenarioContent>
					<path>
						<step name="FA001.1 Al dar clic en el icono Eliminar el sistema mostrará el sig. mensaje de advertencia: &quot;Esta seguro que desea eleminar el archivo seleccionado&quot;" guid="{0D3B029A-998D-4c32-8D18-17E92B3DC22C}" level="1" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="FA001.2 Si el actor presiona el botón Aceptar, el archivo será eliminado, y presiona Cancelar el sistema no realizará ninguna acción." guid="{160EC7EB-5A30-452c-84D6-0EC7B54C2FA9}" level="2" uses="" useslist="" result="" state="" trigger="1" link=""/>
