<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.DebugPhaseListener</phase-listener>
	</lifecycle>

	<application>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>bundles.messages</base-name>
			<var>msg</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>bundles.messages_notificacion</base-name>
			<var>msgNotificacion</var>
		</resource-bundle>

		<message-bundle>bundles.customMessages</message-bundle>

	</application>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ventanillaConfiguracion</from-outcome>
			<to-view-id>/views/ventanilla/catalogos/configuracionCatalogo.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>pruebaFirmaElectronica</from-outcome>
			<to-view-id>/views/comun/firmaElectronica/llamadaFirmaElectronica.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>solicitudRegistroWizard</from-outcome>
			<to-view-id>/views/registro/solicitudRegistro/solicitudRegistroWizard.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<navigation-case>
			<from-outcome>activacionRegistro</from-outcome>
			<to-view-id>/views/registro/activacion/activacion.xhtml</to-view-id>
			<redirect />
		</navigation-case>


	</navigation-rule>


</faces-config>